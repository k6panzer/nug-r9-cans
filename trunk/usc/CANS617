end ClearPage3

function ClearPage4() is null
 $clear(c.cans.commdate,c.cans.school3m,c.cans.grade,c.cans.arrest,c.cans.restype,c.cans.resother1)
 if `$today - c.cans.date` < 30
  $clear(c.cans.slate,c.cans.slateoth)
 endif 
end ClearPage4
