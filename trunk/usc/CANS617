start CANS617(parmfile,option,client,retcode)
 %include c_incmaps
 %allow_nonmapped_io
 %maptest
 %include c_cldefLL
 %include c_clgetLL
 %include inc_CANS617 

 Title = "CANS Report" 
 RptTitle = "CANS Report" 
 ReportID = "CANS"  
 
 if $lc(option) = "reportonly" or $lc(option) = "bigreport"
  if $dbread(ClientDB, $regid, c.cans.rec, c.cans.date) < 2
   if snap_handle:sn_viewLatest(ReportID,Parmfile,$regid,ClientDB,$ScriptID) = 0
    retcode = "0"
   endif
  endif
  retcode = "0"
  return
 endif 

 if {"lib_COMMON"}StaffCanDo($scriptid,"Y") = 1
  return
 endif

 if Validate() = 1
  {"lib_CANS"}DisplayErrors()
 ' return
 endif

 %global assessments
 assessments[] is x
 {"lib_CANS"}Allow_Assesment($regid,assessments[]) 

 %global bgcolor
 bgcolor is x 
 %define table_wbgcolor "width='100%' cellspacing='0' cellpadding='0' bgcolor='" + bgcolor + "'"
 %define table_nobgcolor "width='100%' cellspacing='0' cellpadding='0'"  
 %define row_color1 "bgcolor='LightSkyBlue'"
 %define row_color2 "bgcolor='White'"
 %define row_color3 "bgcolor='PaleGreen'"
 %define col_color_multi "bgcolor='LightGrey'"
 c.cans.cg.na = "N"
 Page is x
 %global UM_StaffYN
 UM_StaffYN is x
 UM_StaffYN = {"lib_COMMON"}UMStaff()
 if UM_StaffYN = "Y"
  if c.cans.compd !dp and c.cans.type dp
   rc = $dbread(02, $regid, cans_dstlist)
   Page = "Authorize"
  else
   Page = "1"
  endif
 endif
 do while 1 = 1
  select Page
   case "1" Page1()
			select $endbutton
			 case "SUBMIT" Page = "Validate1"
			 case other if {"lib_COMMON"}ConfirmExit() = "Y"   
                         return
                        endif
			endselect 
   case "2" Page2()
			select $endbutton
			 case "SUBMIT" Page = "Validate2"
			 case other Page = "1"
			endselect			
   case "3" Page3()
			select $endbutton
			 case "SUBMIT" Page = "Validate3"
			 case other Page = "2"
			endselect
   case "3" Page4()
			select $endbutton
			 case "SUBMIT" Page = "Validate4"
			 case other Page = "3"
			endselect
   case "Validate1" select Validate1()
					 case 0 select CanSkipPage2() 
					         case "Y" Page = "4"
							 case "N" Page = "2"
							endselect
					 case 1 Page = "1"
					        {"lib_CANS"}DisplayErrors()
					endselect										  
   case "Validate2" select Validate2()
					 case 0 select CanSkipPage3() 
					         case "Y" Page = "4"
							 case "N" Page = "3"
							endselect
					 case 1 Page = "2"
					        {"lib_CANS"}DisplayErrors()
					endselect
   case "Validate3" select Validate3()
					 case 0 Page = "4"
					 case 1 Page = "3"
					        {"lib_CANS"}DisplayErrors()  
					endselect
   case "Validate4" select Validate4()
					 case 0 Page = "Report"
					 case 1 Page = "4"
					        {"lib_CANS"}DisplayErrors()
					endselect
   case "ValidateReport" select ValidateReport()
                          case 0 if SaveData() = 0
					             Page = "Exit"
								endif 
                          case 1 Page = "Report"
						         {"lib_CANS"}DisplayErrors()
                         endselect			
   case "ValidateALOC" select ValidateALOC()
                        case 0 Page = "Report"
                        case 1 Page = "Authorize"
                               {"lib_CANS"}DisplayErrors()
                       endselect					
   case "Report"    Report(RptTitle,ReportID,page)
					select $endbutton
					 case "SUBMIT" Page = "ValidateReport"
					 case "CANCEL" Page = "4"
					 case other if SaveData() = 0
					             Page = "Exit"
								endif 
					endselect
   case "Authorize" c.cans.rloc = RLOC()
                    REPORT(RptTitle,ReportID,page)
                    select $endbutton
					 case "SUBMIT" Page = "ValidateALOC"
					 case "CANCEL" if {"lib_COMMON"}ConfirmExit() = "Y"   
                                    return
                                   endif
					endselect
   case "Exit" return					
  endselect 
 enddo
end CANS617

function Report(RptTitle,ReportID,mode) is null
 RptTitle is x
 ReportID is x
 mode     is x
 if UM_StaffYN = "Y"
  select mode
   case "Report"    $submitopt("off", "")
   case "Authorize" $submitopt("off", "Authorize")
  endselect
 else
  $submitopt("off", "Send to UM")
 endif
 $cancelopt("off", "Back") 
 $form("Report")
  {"lib_COMMON"}ClientPageHeader($regid,"Y")
  $style("siginfotag")
  $br(){"lib_CANS617"}ReportHdr(c.cans.date,c.cans.type)
  $br(){"lib_CANS617"}OtherQuestionRpt(q_text_sped,c.cans.sped,q_text_extdrev,c.cans.extdrev,q_text_foster,c.cans.foster,q_text_riskplc,c.cans.riskplc)
  $br(){"lib_CANS617"}ChildRiskRpt(c.cans.cr.suicid,q_text_cr_smutil,c.cans.cr.smutil,q_text_cr_oslfhr,c.cans.cr.oslfhr,
