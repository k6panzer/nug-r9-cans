    $col()$text(q_text_jj_parinf)                                                                          
    $col("center")$radio(c.cans.jj.parinf,"","0")                                                                
    $col("center")$radio(c.cans.jj.parinf,"","1")                                                                
    $col("center")$radio(c.cans.jj.parinf,"","2")                                                                
    $col("center")$radio(c.cans.jj.parinf,"","3")                                                                
   $row(,row_color3)                                                                                 
    $col()$text(q_text_jj_envinf)                                                                          
    $col("center")$radio(c.cans.jj.envinf,"","0")                                                                
    $col("center")$radio(c.cans.jj.envinf,"","1")                                                                
    $col("center")$radio(c.cans.jj.envinf,"","2")                                                                
    $col("center")$radio(c.cans.jj.envinf,"","3")                                                                
  $endtable($funcname)                                                                                            
 $endfieldset()                                                                                                                                                                                                                    
end jjustice                                                

