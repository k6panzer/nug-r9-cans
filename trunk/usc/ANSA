end ClearPage34

function AssessmentStaff() is null
 if StaffSelectDCT dp
  $table($funcname)
   $row()
    $col("right",,"50%")$text("Staff ID: ","datatag")
    $col()$dropboxdct(c.ansa.staff,StaffSelectDCT,,,,"Y")
  $endtable($funcname)
 endif 
end AssessmentStaff

function CrisisOverride() is null
 c.ansa.rloc = "100"
 $clear(c.ansa.dloc,c.ansa.loc_dev)
'Page1 Values
'Risk
 if c.ansa.r.suicid = "0"
  $clear(c.ansa.sr.idea, c.ansa.sr.intent, c.ansa.sr.plan, c.ansa.sr.hist, c.ansa.sr.ffsui)
 endif
 if c.ansa.r.dangot = "0"
  $clear(c.ansa.d.frusman,c.ansa.d.host,c.ansa.d.pthink,c.ansa.d.gainang,c.ansa.d.vthink,c.ansa.d.potent,c.ansa.d.respon,c.ansa.d.csctrl,c.ansa.d.tinv)
 endif

 c.ansa.r.smutil  = "0"
 c.ansa.r.oslfhr  = "0"
 c.ansa.r.exploit = "0"
 c.ansa.r.gamble  = "0"
 c.ansa.r.sexagr  = "0"
 c.ansa.r.crimbe  = "0"
 
'Behavioral
 c.ansa.b.psychs  = "0"
 c.ansa.b.depres  = "0"
 c.ansa.b.anxity  = "0"
 c.ansa.b.mania   = "0"
 c.ansa.b.implsv  = "0"
 c.ansa.b.inter   = "0"
 c.ansa.b.antisoc = "0"
 c.ansa.b.adjtrm  = "0"
 c.ansa.b.angctr  = "0"
 c.ansa.b.eatdst  = "0"
 if c.ansa.b.subabs = "0"
   $clear(c.ansa.sa.severe,c.ansa.sa.durati,c.ansa.sa.stgrec,c.ansa.sa.perinf,c.ansa.sa.envinf,c.ansa.sa.reccom)
 endif

'LifeDomain
 c.ansa.ld.phymed = "0"
 c.ansa.ld.family = "0"
 c.ansa.ld.employ = "9"
 c.ansa.ld.socfun = "0"
 c.ansa.ld.recret = "0"
 c.ansa.ld.sexdev = "0"
 c.ansa.ld.living = "0"
 c.ansa.ld.restab = "0"
 c.ansa.ld.legal  = "0"
 c.ansa.ld.sleep  = "0"
 c.ansa.ld.decisi = "0"
 c.ansa.ld.invrec = "0"		 
 c.ansa.ld.trans  = "0"

'Family/Care Giver
 c.ansa.fc.na     = "9"
 c.ansa.fc.phlth  = "9"
 c.ansa.fc.invcre = "9"
 c.ansa.fc.know   = "9"
 c.ansa.fc.sfres  = "9"
 c.ansa.fc.famst  = "9"
 c.ansa.fc.safe   = "9"

'Strengths
 c.ansa.st.fam    = "0"
 c.ansa.st.soccon = "0"
 c.ansa.st.opti   = "0"
 c.ansa.st.tal    = "0"
 c.ansa.st.edu    = "9"
 c.ansa.st.vol    = "0"
 c.ansa.st.jobhi  = "0"
 c.ansa.st.relig  = "0"
 c.ansa.st.comcon = "0"
 c.ansa.st.natsup = "0"
 c.ansa.st.resil  = "0"
 c.ansa.st.resour = "0"

'Culture/Acculturation
 c.ansa.ac.langua = "0"
 c.ansa.ac.identy = "0"
 c.ansa.ac.ritual = "0"
 c.ansa.ac.culstr = "0"

'Hospital
 c.ansa.hs.num180 = "0"
 c.ansa.hs.numl30 = "0"
 c.ansa.hs.numg30 = "0"

'Crisis
 c.ansa.cr.episds = "0"
 
 ClearPage2() 
 ClearPage34()
end CrisisOverride
