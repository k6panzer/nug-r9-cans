     $col("center")$radio(c.ansa.vc.jtime,,$castx(radio_index))
 	enddo     
   $row(,row_color3)
    $col()$text(q_text_vc_jatt)
    $col("center")$radio(c.ansa.vc.jatt,,"9")
	radio_index = -1
