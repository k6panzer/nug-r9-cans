'Trauma Module Report
function TraumaRpt() is null
 $table($funcname,,"width='100%' cellspacing='0' cellpadding='2' bgcolor='White'  style='border-style: solid; border-width: 1px'")
  $row( ,col_color_multi)
   $col("center",,,"2"  )$text("Trauma Module", "datatag")
