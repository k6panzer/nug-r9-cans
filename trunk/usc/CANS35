	 $col()$fieldset("Family/Caregiver",,"datatag")
	        CareGiverScoring()
		   $endfieldset()
	 $col()
    $row()
     $col(,,"50%")FamilyCareGive()
     $col()
   endif
  $endtable($funcname)	
 $sendform($funcname)
end Page2

