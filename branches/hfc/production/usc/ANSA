  $row()                                       
   $col()$text(q_text_tr_intrus)
   $col("center")$text(c.ansa.tr.intrus)
  $row()                                       
   $col()$text(q_text_tr_attach)
   $col("center")$text(c.ansa.tr.attach)
  $row()                                       
   $col()$text(q_text_tr_dissoc)
   $col("center")$text(c.ansa.tr.dissoc)
 $endtable($funcname)
end TraumaRpt

