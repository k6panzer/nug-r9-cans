	radio_index = -1
	do while radio_index++ < 1							
     $col("center")$radio(c.ansa.r.crimbe,,$castx(radio_index))
    enddo
	radio_index = 1
	do while radio_index++ < 3							
     $col("center",,,,,col_color_multi)$radio(c.ansa.r.crimbe,,$castx(radio_index))
    enddo
