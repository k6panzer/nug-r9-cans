   $row()
    $col("center",,"50%")
	radio_index = -1
	do while radio_index++ < 3
     $col("center",,"10%")$text(radio_index)
	enddo
   $row()
    radio_index = 0
    do while radio_index++ < 5
     $col()$tag("<hr/>")
    enddo
   $row(,row_color3)
    $col()$text(q_text_sr_idea)
	radio_index = -1
