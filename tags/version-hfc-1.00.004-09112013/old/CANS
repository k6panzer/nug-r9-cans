function CanSkipPage2() is x
 CanSkipPage2 = "Y"
 if $num(c.cans.cr.dangot) > 0 or 
    $num(c.cans.cr.sexagr) > 0 or 
	$num(c.cans.cr.runawy) > 0 or 
	$num(c.cans.cr.delqcy) > 0 or 
	$num(c.cans.cr.firset) > 0 or 
	$num(c.cans.cb.adjtrm) > 0 or 
	$num(c.cans.cb.subabs) > 0 or
	$num(c.cans.ld.school) > 0 or 
	$num(c.cans.ld.develp) > 0 or 
	$num(c.cans.cg.safety) > 0 or 
	c.cans.ps.hosptl = "1"
  CanSkipPage2 = "N"
 endif	
end CanSkipPage2

function CanSkipPage3() is x
 CanSkipPage3 = "Y"
 if $num(c.cans.tr.sexabs) > 0
  CanSkipPage3 = "N"
 endif
end CanSkipPage3

function ChildRiskScoring() is null
 $table("CR",,`table_wbgcolor`)  
  $row()
   $col(,,"50%")$text("0 = no evidence","hdrdata")
   $col()$text("1 = history, watch/prevent","hdrdata")
  $row()
   $col()$text("2 = recent, act","hdrdata")
   $col()$text("3 = acute, act immediately","hdrdata")
  $row()
   $col("center",,,"2") $tag(hr)
 $endtable("CR")
end ChildRiskScoring

function CBScoring() is null
 $table("CB",,`table_wbgcolor`)
  $row()
   $col()$text("0 = no evidence","hdrdata")
         $br()$text("1 = history or sub-threshold, watch/prevent","hdrdata")
         $br()$text("2 = causing problems, consistent with diagnosable disorder","hdrdata")
         $br()$text("3 = causing severe/dangerous problems","hdrdata")
  $row()
   $col("center",,,"2") $tag(hr)
 $endtable("CB")
end CBScoring

function LDScoring() is null
 $table("LD",,`table_wbgcolor`)
  $row()
   $col(,,"50%")$text("0 = no evidence of problems","hdrdata")
   $col()$text("1 = history, mild","hdrdata")
  $row()
  $col()$text("2 = moderate","hdrdata")
   $col()$text("3 = severe","hdrdata")
  $row()
   $col("center",,,"2") $tag(hr)
 $endtable("LD")
end LDScoring

function CareGiverScoring() is null
 $table("CG",,`table_wbgcolor`)  
  $row()
   $col(,,"50%")$text("0 = no evidence","hdrdata")
   $col()$text("1 = minimal needs","hdrdata")
  $row()
   $col()$text("2 = moderate needs","hdrdata")
   $col()$text("3 = severe needs","hdrdata")
  $row()
    $col("center",,,"2") $tag(hr)
 $endtable("CG")
end CareGiverScoring

function PHScoring() is null
 $table("PH",,`table_wbgcolor`)  
  $row()
   $col()$text("NA = Not Applicable","hdrdata")
   $col()$text("0 = No","hdrdata")
   $col()$text("1 = Yes","hdrdata")
  $row()
   $col("center",,,"3")$tag(hr)
 $endtable("PH")
end PHScoring
