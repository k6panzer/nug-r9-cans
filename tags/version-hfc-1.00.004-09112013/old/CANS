start CANS(parmfile,option,client,retcode)
 %include c_incmaps
 %allow_nonmapped_io
 %maptest
 %include c_cldefLL
 %include c_clgetLL
 %include inc_CANS 

 Title = "CANS Report" 
 RptTitle = "CANS Report" 
 ReportID = "CANS"  
 
 if option = "reportonly" or option = "bigreport"
  if $dbread(ClientDB, $regid, c.cans.rec, c.cans.date) < 2
   if snap_handle:sn_viewLatest(ReportID,Parmfile,$regid,ClientDB,$ScriptID) = 0
    retcode = "0"
   endif
  endif
  retcode = "0"
  return
 endif 

 if {"lib_COMMON"}StaffCanDo($scriptid,"Y") = 1
  return
 endif

 %global bgcolor
 bgcolor is x 
 %define table_wbgcolor "width='100%' cellspacing='0' cellpadding='0' bgcolor='" + bgcolor + "'"
 %define table_nobgcolor "width='100%' cellspacing='0' cellpadding='0'"  
 %define row_color1 "bgcolor='LightSkyBlue'"
 %define row_color2 "bgcolor='White'"
 %define row_color3 "bgcolor='PaleGreen'"
 %define col_color_multi "bgcolor='LightGrey'"
 c.cans.cg.na = "N"
 Page is x
 Page = "1"
 do while 1 = 1
  select Page
   case "1" Page1()
            if $endbutton = "SUBMIT"
			 Page = "Validate1"
			else
			 if {"lib_COMMON"}ConfirmExit() = "Y"   
              return
             endif	
			endif 
   case "2" Page2()
            if $endbutton = "SUBMIT"
			 Page = "Validate2"		
			else
			 Page = "1" 
			endif
   case "3" Page3()
            if $endbutton = "SUBMIT"
			 Page = "Validate3"
			else
             Page = "2"
			endif   			
   case "Validate1" if Validate1() = 0
                     if CanSkipPage2() = "Y"
                      Page = "Report"
					 else
					  Page = "2"
					 endif  
					else
					 Page = "1"
					 DisplayErrors()
					endif  
   case "Validate2" if Validate2() = 0
                     if CanSkipPage3() = "Y"					  
					  Page = "Report"
					 else
					  Page = "3"
					 endif 
					else
					 Page = "2"
					 DisplayErrors()
					endif
   case "Validate3" if Validate3() = 0
                     Page = "Report"
					else
					 Page = "3"
					 DisplayErrors()
					endif
   case "Report"    Report(RptTitle,ReportID)
					select $endbutton
					 case "SUBMIT" if SaveData() = 0
							        Page = "Exit"
								   endif	 
					 case "CANCEL" select CanSkipPage2()
					                case "Y" Page = "1"
					                case "N" select CanSkipPage3()
									          case "Y" Page = "2"
											  case "N" Page = "3"
											 endselect
					               endselect 
					 case other if SaveData() = 0
					             Page = "Exit"
								endif 
					endselect
   case "Exit" return					
  endselect 
 enddo
end CANS

function Report(RptTitle,ReportID) is null
 RptTitle is x
 ReportID is x
 $submitopt("off", "")
 $cancelopt("off", "BACK")
 $form("Report")
  {"lib_COMMON"}ClientPageHeader($regid,"Y")
  $style("siginfotag")
  $br()ChildRiskRpt()
  $br()BehEmoRpt()
  $br()LifeDomainRpt()
  $br()CareGiveRpt()
  $br()ChildStrRpt()
  $br()AccultRpt()
  $br()PsyHospRpt()
  $br()CrisisRpt()
  {"l_snapLibSC"}sn_CreateReport(RptTitle,,,ReportID)
 $sendform("Report")
end Report

function SaveData() is i
 $dblock()
 SaveData = $dbpoint(02, $regid, c.cans.rec)	                          
 c.cans.staff       = $operstaffid
 c.cans.ver         = "0.02"
 c.cans.date        = $today
 SaveData = $dbadddst(02, $regid, cans_dstlist)
  select SaveData
   case 0     $brmsg("Data Successfully Added to Database!",1,"W","Process Complete")
   case other $brmsg(`"Error Saving Data,  RC=" + SaveData`,1,"W","Error Saving")
  endselect
end SaveData

'Life Domain
function LifeDomain() is null
