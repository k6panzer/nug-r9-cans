   $col(,,"50%")                               
  $row()                                      
   $col()        $text(q_text_cb_implsv)
   $col("center")        $text(c.cans.cb.implsv)
   $col()                                      
  $row()                                      
   $col()        $text(q_text_cb_depres)
   $col("center")        $text(c.cans.cb.depres)
   $col()                                      
  $row()                                      
   $col()        $text(q_text_cb_anxity)
   $col("center")        $text(c.cans.cb.anxity)
   $col(,,"50%")                               
